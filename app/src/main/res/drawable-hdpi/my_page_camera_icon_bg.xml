<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners
        android:radius="100px"/>
    <solid
        android:color="@color/deep_grey"
        />
    <stroke
        android:width="1px"
        android:color="@color/lightgrey"/>
    <padding
        android:bottom="3dp"
        android:top="3dp"
        android:right="3dp"
        android:left="3dp"
        />
    <size
        android:width="40dp"
        android:height="40dp"
        />

</shape>